<Window x:Class="Laminatte.Windows.AdminWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Laminatte.Windows"
        mc:Ignorable="d"
        Title="AdminWindow" Height="450" Width="800">
    <Grid>
        <Image Source="/Resources/fon.png" Stretch="UniformToFill"/>
        <StackPanel>
            <Label Content="Пользователи:" Foreground="White" HorizontalContentAlignment="Center" Margin="5"/>
            <ScrollViewer Height="300" Margin="5">
                <StackPanel x:Name="UsersScrollViewer" Width="460">

                </StackPanel>
            </ScrollViewer>
            <Button x:Name="CreateUserBTN" Content="Создать нового пользователя" Margin="5" Width="250" Click="CreateUserBTN_Click"/>
            <Button x:Name="ExitBTN" Content="Назад" Margin="5" Width="250" Click="ExitBTN_Click"/>
        </StackPanel>
    </Grid>
</Window>
